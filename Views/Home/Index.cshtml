<style>
 

              * {
        box-sizing: border-box;
        padding: 0;
        margin: 0;
      }
      .video {
        position: relative;
      
        
      }

      .video video {
        width: 100%;
       
       
      }
      .video .text {
        width: 35%;
        position: absolute;
        top: 50%;

        transform: translateY(-50%);
        padding: 50px;
        background-color: rgba(211, 205, 212, 0.7);
        text-align: center;
      }
      .video .text h2 {
        margin-bottom: 30px;
        text-transform: uppercase;
      }
      .video .text p {
        margin-bottom: 20px;
        font-size: 20px;
      }
      .video .text button {
       z-index:999;

      }
  
     .video{
       margin-top: 20px;
     }
        }

    /* Style for the total div */
    .total {
        margin-top: 20px;
    }

 

  .features{
      padding-top: 20px;
      padding-bottom: 60px;
        background-color: #f5f1ed;

  }

  

 
</style>

